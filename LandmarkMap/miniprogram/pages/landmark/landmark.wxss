.page {
  position: relative; /* 使伪元素相对于.page定位 */
}

.page::before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  z-index: -1; /* 设置z-index使背景层在内容层之上 */
  background: #e2dfdf8d;
  filter: blur(3px);
  z-index: -1;
}

.swiper-container {
  width: 100%;
  height: 200px;
}

.swiper-image {
  width: 100%;
  height: 100%;
}

.inputbar input {
  padding-left: 30rpx;
  padding-right: 30rpx;
  top: 50%;
}

.input_text {
  font-family: CustomFontFamily;
  width: 90%;
  height: 90%;
  top: 50%;
  border-radius: 10rpx;
  padding-left: 10rpx;
  font-size: 30rpx;
}

.landmark {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 30rpx;
}
